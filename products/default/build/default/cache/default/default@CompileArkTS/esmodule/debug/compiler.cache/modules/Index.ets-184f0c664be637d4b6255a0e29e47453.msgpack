          .margin({ bottom: 64 }) // 閫傞厤搴曢儴绯荤粺鏍?
          .width('100%')
          .height('100%')
          .barHeight(80)
          .barMode(BarMode.Fixed)
          .onChange((index: number) => {
            this.currentTabIndex = index;
          })
        }
        .width('100%')
        .height('100%')
        .backgroundColor('#F5F5F5')
      }
      .width('100%')
      .height('100%')
    } else {
      // 鏈櫥褰曪紝鏄剧ず鐧诲綍椤甸潰
      LoginPage({
        onLoginSuccess: () => {
          this.checkLoginStatus();
        }
      })
    }
  }
}








